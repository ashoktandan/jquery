                //    }
                //}
                


                //for (var z = 0; z < p.length; z++) {
                //    if (p[z].toString() == pat.toString()) {
                //        $('table').append('<tr><td>'+p[z]+'</td><td>' + arr[z + 1] + '</td></tr>');
                //    }
                //    else if (p[z].toString != pat.toString) {
                //        alert('not');
                //    }
                //}
            })
        });
