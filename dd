        $(document).ready(function () {
            arr = [];
            p = [];
            arr =$('#content').text().split('Reported on');
            for (var i = 1; i < arr.length; i++) {
           
                $('table').append('<tr><td class="date">' + arr[i].substring(1, 10) + '</td><td>' + arr[i].replace("-------------------------------- ", "") + '</td></tr>');              
            }
            $('#dat').keypress(function () {
                var l=$('#dat').val().length;
                var pat = $('#dat').val();
                $('.date').each(function () {
                    if ($(this).text().substring(0, l + 1) != pat) {
                        $(this).parent().parent().hide();
                    }
                });
                //$('table').empty();
                //for (var i = 0; i < arr.length; i++) {
